        "url": "https://github.com/cubbit/web-file-stream/issues"
    },
    "homepage": "https://github.com/cubbit/web-file-stream#readme",
    "devDependencies": {
        "@types/jest": "^24.0.9",
        "@types/node": "^11.10.5",
        "fs-extra": "^7.0.1",
        "jest": "^24.3.1",
        "rimraf": "^2.6.3",
        "ts-jest": "^24.0.0",
        "tslint": "^5.13.1",
        "typescript": "^3.3.3333"
    }
