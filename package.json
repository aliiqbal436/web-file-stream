{
    "name": "@cubbit/web-file-stream",
    "version": "1.0.0",
    "private": true,
    "description": "Node's file stream APIs for the web",
    "main": "index.js",
    "scripts": {
        "lint": "tslint --project tsconfig.json -c tslint.json src/**/*.ts",
        "pretest": "npm run lint && tsc",
